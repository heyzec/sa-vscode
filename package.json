{
  "name": "sa-vscode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.84.2"
  },
  "main": "./out/extension.js",
  "activationEvents": [],
  "contributes": {
    "commands": [
      {
        "command": "helloworld.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^22.5.4",
    "@types/vscode": "^1.93.0",
    "typescript": "^5.6.2"
  }
}
