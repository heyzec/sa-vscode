{
  "name": "sa-vscode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.84.2"
  },
  "main": "./out/extension.js",
  "activationEvents": [],
  "contributes": {
    "commands": [
      {
        "command": "sa-vscode.react",
        "title": "Start a React page"
      }
    ]
  },
  "scripts-info": {
    "compile": "Build bundles",
    "format": "Format source code",
    "watch": "Recompile source code on change"
  },
  "scripts": {
    "compile": "node ./esbuild.js",
    "format": "yarn prettier . --write",
    "vscode:prepublish": "yarn run compile",
    "watch": "node ./esbuild.js --watch"
  },
  "devDependencies": {
    "@types/node": "^22.5.4",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/vscode": "^1.93.0",
    "esbuild": "^0.23.1",
    "husky": "^8.0.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
